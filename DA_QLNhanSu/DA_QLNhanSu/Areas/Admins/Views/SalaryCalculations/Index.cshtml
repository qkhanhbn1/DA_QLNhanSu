            <td>
                @Html.DisplayFor(modelItem => item.IdTimesheetNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdeNavigation.Ide)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Ids">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Ids">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Ids">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
